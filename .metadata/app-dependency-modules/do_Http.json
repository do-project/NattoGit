{
  "ID": "do_Http",
  "IsContainer": "0",
  "Type": "MM",
  "Name": "http/https请求服务",
  "Version": "Internal",
  "Description": "支持http/https请求服务",
  "Icon": "https://do-store.oss-cn-beijing.aliyuncs.com/D/20160121/do_Http/http.jpg",
  "IOS": {
    "Support": 1,
    "MinVersion": "7.0"
  },
  "Android": {
    "Support": 1,
    "MinVersion": "4.0"
  },
  "Windows": {
    "Support": 1,
    "MinVersion": "10"
  },
  "WindowsPhone": {
    "Support": 1,
    "MinVersion": "10"
  },
  "Html": {
    "Support": 1,
    "MinVersion": "5.0"
  },
  "Properties": [
    {
      "ID": "body",
      "Name": "请求数据",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "请求数据，method为get、delete时不支持",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "contentType",
      "Name": "内容类型",
      "Type": "String",
      "DefaultValue": "text/html",
      "EditType": "1",
      "Group": "",
      "Description": "内容类型",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "method",
      "Name": "请求方式",
      "Type": "String",
      "DefaultValue": "get",
      "EditType": "1",
      "Group": "",
      "Description": "http请求方式 GET/POST/PUT/PATCH/DELETE",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "responseEncoding",
      "Name": "字符集格式",
      "Type": "String",
      "DefaultValue": "utf-8",
      "EditType": "1",
      "Group": "",
      "Description": "设置服务端返回内容的解码格式，通常与服务端开发人员约定，当属性有值时优先以该属性值格式解码，当不设置该属性时以默认值utf-8格式解码",
      "Design": {
        "Type": "Text",
        "Data": []
      }
    },
    {
      "ID": "timeout",
      "Name": "请求超时",
      "Type": "Number",
      "DefaultValue": "5000",
      "EditType": "1",
      "Group": "",
      "Description": "请求服务器超时时间,单位是毫秒",
      "Design": {
        "Type": "UInteger",
        "Data": []
      }
    },
    {
      "ID": "url",
      "Name": "地址",
      "Type": "String",
      "DefaultValue": "",
      "EditType": "1",
      "Group": "",
      "Description": "发送服务器请求地址",
      "Design": {
        "Type": "Richtext",
        "Data": []
      }
    }
  ],
  "Events": [
    {
      "ID": "fail",
      "Description": "请求出错事件",
      "ReturnType": "Node",
      "ReturnDesc": "返回值包含两个节点{status:'http错误码' ,message:'错误的信息'}"
    },
    {
      "ID": "progress",
      "Description": "响应请求事件",
      "ReturnType": "Node",
      "ReturnDesc": "返回值包含两个节点{currentSize:'1221',totalSize:'23234245'},单位为kB"
    },
    {
      "ID": "result",
      "Description": "请求结束事件",
      "ReturnType": "Node",
      "ReturnDesc": "返回值包含两个节点{status:'http状态码' ,data:'服务端返回信息'}"
    },
    {
      "ID": "success",
      "Description": "请求成功事件",
      "ReturnType": "String",
      "ReturnDesc": "返回http接收到的数据"
    }
  ],
  "syncMethodes": [
    {
      "ID": "download",
      "Name": "下载",
      "Description": "下载文件",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "path",
          "Name": "保存地址",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "下载到本地的文件的全路径，只支持data://"
        }
      ]
    },
    {
      "ID": "form",
      "Name": "上传表单",
      "Description": "支持同时上传多个文件和字符串",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "data",
          "Name": "上传内容",
          "Type": "Node",
          "Required": 1,
          "DefaultValue": "",
          "Description": "需要上传的数据内容，如{'files':[{'key':'file1','value':'data://1.png'}...],'texts':[{'key':'text1','value':'data://1.png'}...]}，其中标识key只能是texts或者files，为text时value表示字符串值；为file时value表示需要上传的源文件地址，可以是data://目录"
        }
      ]
    },
    {
      "ID": "getResponseHeader",
      "Name": "获取请求头",
      "Description": "",
      "ReturnType": "String",
      "ReturnDesc": "返回最后一次成功返回的http respose的header里某项属性的值",
      "Paras": [
        {
          "ID": "key",
          "Name": "http请求的header对应的属性",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": ""
        }
      ]
    },
    {
      "ID": "request",
      "Name": "发送请求",
      "Description": "",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": []
    },
    {
      "ID": "setRequestHeader",
      "Name": "设置请求头",
      "Description": "",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "key",
          "Name": "http请求的header对应的属性",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": ""
        },
        {
          "ID": "value",
          "Name": "http请求的header对应的值",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": ""
        }
      ]
    },
    {
      "ID": "upload",
      "Name": "上传",
      "Description": "上传文件",
      "ReturnType": "",
      "ReturnDesc": "",
      "Paras": [
        {
          "ID": "path",
          "Name": "源地址",
          "Type": "String",
          "Required": 1,
          "DefaultValue": "",
          "Description": "需要上传的源文件地址，可以是data://目录"
        },
        {
          "ID": "name",
          "Name": "名称",
          "Type": "String",
          "Required": 0,
          "DefaultValue": "file",
          "Description": "(表单方式的上传)定义input元素的名称"
        }
      ]
    }
  ],
  "asyncMethodes": []
}